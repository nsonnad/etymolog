div#graph
  .node-link
    fill none
    stroke $light-pink
    stroke-width 1px
    stroke-opacity 0.8
    &.active-source
      stroke $hilight-source
      stroke-opacity 1.0
    &.active-target
      stroke $hilight-target
      stroke-opacity 1.0
  .node-circle
    fill $dark-pink
  .arrow-marker
    fill $light-pink
